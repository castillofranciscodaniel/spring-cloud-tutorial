package com.springboot.app.items.models.util;

import org.springframework.stereotype.Component;

import lombok.Data;

@Data
@Component
public class OkBody {

	private String message;

	public OkBody() {
		super();
	}

	public OkBody(String message) {
		super();
		this.message = message;
	}

	public OkBody setMessage(String message) {
		this.message = message;
		return this;
	}

}
